.gitignore

# Cache and logs (Symfony2)
app/cache/*
app/logs/*
!app/cache/.gitkeep
!app/logs/.gitkeep

# Cache and logs (Symfony3)
var/cache/*
var/logs/*
!var/cache/.gitkeep
!var/logs/.gitkeep

# Parameters
app/config/parameters.yml
app/config/parameters.ini


# Assets and user uploads
web/bundles/
web/uploads/
web/media/cache/
web/.htaccess

# PHPUnit
app/phpunit.xml
phpunit.xml

# Build data
build/

# Composer PHAR
composer.phar

